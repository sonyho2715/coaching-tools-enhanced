# Database Configuration
# For Railway: This will be automatically provided by Railway PostgreSQL
# For local development: Use your local PostgreSQL connection string
DATABASE_URL=postgresql://user:password@localhost:5432/coaching_tools

# Supabase Configuration (Optional - can use Railway Postgres instead)
# Get these from https://supabase.com/dashboard/project/_/settings/api
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Stripe Configuration
# Get these from https://dashboard.stripe.com/apikeys
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here
STRIPE_SECRET_KEY=sk_test_your_secret_key_here

# Server Configuration
PORT=3000
NODE_ENV=development
